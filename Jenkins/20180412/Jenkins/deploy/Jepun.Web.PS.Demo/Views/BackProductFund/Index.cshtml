                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>
                                    <select size="6" class="form-control" name="ProductFundNo"></select>
                                    @Html.HiddenFor(model => model.LogSN).Txt()
                                    @Html.HiddenFor(model => model.CID).Txt()
