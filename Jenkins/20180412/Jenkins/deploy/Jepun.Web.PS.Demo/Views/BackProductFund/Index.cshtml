                    @Html.EditorFor(Model => Model.OrderLowCost).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "單筆申購最低金額" })
                    @Html.ValidationMessageFor(model => model.OrderLowCost)
                </div>
            </div>*@
            <div class="form-group">                  
