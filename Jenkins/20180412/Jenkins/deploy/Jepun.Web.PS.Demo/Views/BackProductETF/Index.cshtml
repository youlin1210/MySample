                        <label>投資月報: </label>
                    </div>
                    <div class="col-sm-4" data-jepun-key="1">
                        <select class="form-control" name="ProductEtfFile1"></select>
                        @Html.TextBoxFor(Model => Model.InputFile_1, new { type = "file", accept = "application/pdf" }).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "檔案" })
                        <button class="btn btn-default" type="button" name="BtnInsFile1">新增檔案</button>
                        <button class="btn btn-primary" type="button" name="BtnDelFile1">刪除檔案</button>
                    </div>
                    <div class="col-sm-2 control-label">
                        <label>簡式公開說明書: </label>
                    </div>
                    <div class="col-sm-4" data-jepun-key="2">
