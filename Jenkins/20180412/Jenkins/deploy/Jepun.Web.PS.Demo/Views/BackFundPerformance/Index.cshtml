@model Jepun.Web.PS.Models.PsFundPerformanceViewModel
@using Jepun.Web.PS.Comm
@{
    Layout = null;
}
<div class="alert alert-info" role="alert">境內基金績效</div>
@using (Html.BeginForm("", "BackFundPerformance", FormMethod.Post, new { @class = "form-horizontal", role = "form", id = "FormBackFundPerformance", onsubmit = "return false" }))
{
    @Html.ValidationSummary(true)
    <div class="panel panel-primary">
        <div class="panel-body">
            <div class="form-group jepun-upcoming">
                <div class="col-sm-2 control-label">
                    <label>上架日</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(model => model.QryStartDate, new { htmlAttributes = new { max = "2100-12-31" } }).DT(new HtmlAttrs { PlaceHolder = "YYYY / MM / DD", ErrMsg = Jepun.Resource.Lab.PleaseInput + "上架日" })
                    @Html.ValidationMessageFor(model => model.QryStartDate)
                </div>
                <div class="col-sm-2 control-label">
                    <label>下架日</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(model => model.QryEndDate, new { htmlAttributes = new { max = "2100-12-31" } }).DT(new HtmlAttrs { PlaceHolder = "YYYY / MM / DD", ErrMsg = Jepun.Resource.Lab.PleaseInput + "下架日" })
                    @Html.ValidationMessageFor(model => model.QryEndDate)
                </div>
            </div>

            <div class="form-group jepun-upcoming">
                <div class="col-sm-2 control-label">
                    <label>狀態</label>
                </div>
                <div class="col-sm-2">
                    @Html.DropDownListFor(model => model.QryType, Enumerable.Empty<SelectListItem>()).Sel()
                </div>
                <div class="col-sm-2">
                    @Html.DropDownListFor(model => model.QryStatus, Enumerable.Empty<SelectListItem>()).Sel()
                </div>
                <div class="col-sm-2 control-label">
                </div>
                <div class="col-sm-4">
                    <button type="button" class="btn btn-primary" name="BtnQry">查詢</button>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>　標題　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　　異動　　　上稿日期　　狀態　<button type="button" class="btn btn-primary btn-xs" name="QryFlow">歷史流程</button></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="4">
                                    <select size="6" class="form-control" name="FundPerformanceNo"></select>
                                    @Html.HiddenFor(model => model.LogSN).Txt()
                                    @Html.HiddenFor(model => model.CID).Txt()
                                    @Html.HiddenFor(model => model.FID).Txt()
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>資料日期</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(model => model.DT, new { htmlAttributes = new { max = "2100-12-31" } }).DT(new HtmlAttrs { PlaceHolder = "YYYY / MM / DD", ErrMsg = Jepun.Resource.Lab.PleaseInput + "上架日" })
                    @Html.ValidationMessageFor(model => model.DT)
                </div>
                <div class="col-sm-2 control-label">
                    <label>上架日</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(model => model.StartDate, new { htmlAttributes = new { max = "2100-12-31" } }).DT(new HtmlAttrs { PlaceHolder = "YYYY / MM / DD", ErrMsg = Jepun.Resource.Lab.PleaseInput + "上架日" })
                    @Html.ValidationMessageFor(model => model.StartDate)
                </div>
                @*<div class="col-sm-2 control-label">
                        <label>備註</label>
                    </div>
                    <div class="col-sm-4">
                        @Html.TextAreaFor(Model => Model.Remark).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "備註" })
                        @Html.ValidationMessageFor(model => model.Remark)
                    </div>*@
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>下架日</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(model => model.EndDate, new { htmlAttributes = new { max = "2100-12-31" } }).DT(new HtmlAttrs { PlaceHolder = "YYYY / MM / DD", ErrMsg = Jepun.Resource.Lab.PleaseInput + "下架日" })
                    @Html.ValidationMessageFor(model => model.EndDate)
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2"></div>
                <div class="col-sm-4">
                    <button class="btn btn-primary" type="button" name="BtnIns">新增</button>
                    <button class="btn btn-primary" type="button" name="BtnUpd">修改</button>
                    <button class="btn btn-primary" type="button" name="BtnDel">刪除</button>
                </div>
                <div class="col-sm-3">
                    @Html.DropDownListFor(model => model.NextFlow, Enumerable.Empty<SelectListItem>()).Sel()
                    @*@Html.DropDownListFor(model => model.NextFlow, Enumerable.Empty<SelectListItem>()).Sel()
                        @Html.ValidationMessageFor(model => model.NextFlow)*@
                </div>
                <div class="col-sm-3">
                    <button class="btn btn-primary" type="button" name="BtnFlow">確定</button>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-12">
                    <table class="table">
                        <thead>
                            <tr>
                                <th>　基金名稱</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td colspan="4">
                                    <select size="6" class="form-control" name="FundPerformanceFund"></select>
                                    @Html.HiddenFor(model => model.LogSN2).Txt()
                                    @Html.HiddenFor(model => model.Num).Txt()
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>基金</label>
                </div>
                <div class="col-sm-4">
                    @Html.DropDownListFor(model => model.Fund, Enumerable.Empty<SelectListItem>()).Sel()
                    @Html.ValidationMessageFor(model => model.Fund)
                </div>
                <div class="col-sm-2 control-label">
                    <label>基金簡稱</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.Name).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "基金簡稱" })
                    @Html.ValidationMessageFor(model => model.Name)
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>3個月</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.M3).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "3個月" })
                    @Html.ValidationMessageFor(model => model.M3)
                </div>
                <div class="col-sm-2 control-label">
                    <label>6個月</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.M6).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "6個月" })
                    @Html.ValidationMessageFor(model => model.M6)
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>1年</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.M12).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "1年" })
                    @Html.ValidationMessageFor(model => model.M12)
                </div>
                <div class="col-sm-2 control-label">
                    <label>2年</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.M24).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "2年" })
                    @Html.ValidationMessageFor(model => model.M24)
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>3年</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.M36).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "3年" })
                    @Html.ValidationMessageFor(model => model.M36)
                </div>
                <div class="col-sm-2 control-label">
                    <label>5年</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.M60).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "5年" })
                    @Html.ValidationMessageFor(model => model.M60)
                </div>
            </div>
            <div class="form-group">
                <div class="col-sm-2 control-label">
                    <label>今年以來</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.TY).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "今年以來" })
                    @Html.ValidationMessageFor(model => model.TY)
                </div>
                <div class="col-sm-2 control-label">
                    <label>成立以來</label>
                </div>
                <div class="col-sm-4">
                    @Html.EditorFor(Model => Model.CR).Txt(new HtmlAttrs { PlaceHolder = Jepun.Resource.Lab.PleaseInput + "成立以來" })
                    @Html.ValidationMessageFor(model => model.CR)
                </div>
            </div>

            <div class="form-group text-center">
                <div class="col-sm-2"></div>
                <div class="col-sm-8">
                    <button class="btn btn-primary" type="button" name="BtnIns2">新增</button>
                    <button class="btn btn-primary" type="button" name="BtnUpd2">修改</button>
                    <button class="btn btn-primary" type="button" name="BtnDel2">刪除</button>
                </div>
                <div class="col-sm-2"></div>
            </div>
        </div>
    </div>
}
<script src="~/Scripts/Apps/BackFundPerformance.js?@ServiceHelper.CacheBusting()"></script>